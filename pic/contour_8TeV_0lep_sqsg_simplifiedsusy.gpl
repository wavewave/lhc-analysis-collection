#set terminal png 
#set output 'simplifiedsusy_0lep_sqsg_8TeV.png'

set xlabel "Gluino Mass (GeV)"
set ylabel "Squark Mass (GeV)"
set xrange [500:3000] 
set yrange [500:3000]
set zrange [-12:8]
unset key 
unset surface
set contour base
set cntrparam level incremental -20, 20, 20
unset surface
set table 'simplifiedsusy_0lep_sqsg_8TeV_contour.dat'
splot 'simplifiedsusy_sqsg_8TeV_0lep.dat' using 1:2:(log ($3)) with lines lw 1.5 lc rgb "#202020"



